pipeline{
  agent any
  
  stages{
    stage("Build"){
      steps{
        echo "Building..."
      }
    }
    stage("Test"){
      steps{
        echo "Testing..."
      }
    }
    stage("Deploye"){
      steps{
        echo "Deploying..."
      }
    }
    stage("Clone"){
      steps{
        sh """
          pwd
          git clone https://github.com/samiullah553/pipline-as-code.git
          ls
          cat index.html
          """
      }
    }
  }
}
