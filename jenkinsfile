pipeline{
  agent any
  
  stages{
    stage("Testing"){
      steps{
        echo "Hello World"
      }
    }
    stage("Building"){
      steps{
        sh """
          pwd
          git clone https://github.com/samiullah553/pipline-as-code.git
          ls
          cat index.html
        """
      }
    }
  }
}
